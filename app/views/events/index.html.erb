<%- selected(:eventos) %>

<h3 class="titles_index">Eventos</h3>


<% @events.each do |event| %>

    <%=h event.title %>
    <%= link_to 'Show', event %>
    <%= link_to 'Edit', edit_event_path(event) %>
    <%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %>
 
<% end %>

<br /><br />

<%= link_to 'New event', new_event_path %>