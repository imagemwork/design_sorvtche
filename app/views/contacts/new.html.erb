<%- title("Contato") %>
<%- selected(:contato) %>




	<% form_for(@contact) do |f| %>
	
	
		
		<%= f.error_messages %>
		

		

	    <%= f.label :nome %><br />
	    <%= f.text_field :nome, :class => "input-text" %>

	    <%= f.label :assunto %><br />
	    <%= f.text_field :assunto, :class => "input-text" %>

	    <%= f.label :email %><br />
	    <%= f.text_field :email, :class => "input-text" %>

	    <%= f.label :mensagem %><br />
	    <%= f.text_area :mensagem, :class => "input-text", :rows => 10, :maxlenght => "130" ,:style => "overflow: auto;" %>

	    <%= f.submit 'Enviar', :class => "button-login" %>

	
	

	
	
	<% end %>
	
